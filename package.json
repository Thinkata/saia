{
  "name": "saia-level1",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc -p tsconfig.json",
    "start": "tsx src/index.ts",
    "simulate": "tsx scripts/simulate.ts",
    "test": "npm run test:unit && npm run test:system",
    "test:unit": "vitest --config vitest.config.ts run tests/unit --coverage",
    "test:system": "vitest --config vitest.config.ts run tests/system",
    "dashboard:dev": "npm --prefix dashboard run dev",
    "dashboard:build": "npm --prefix dashboard run build",
    "dashboard:generate": "npm --prefix dashboard run generate",
    "dashboard:preview": "npm --prefix dashboard run preview"
  },
  "dependencies": {
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "openai": "^4.59.1",
    "pino": "^9.3.2",
    "yaml": "^2.8.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.30",
    "@vitest/coverage-v8": "^2.1.9",
    "tsx": "^4.19.1",
    "typescript": "^5.6.3",
    "vitest": "^2.1.4"
  },
  "engines": {
    "node": ">=18.18.0"
  }
}
